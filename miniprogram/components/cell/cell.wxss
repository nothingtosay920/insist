/* components/cell/cell.wxss */

.cell {
  border:1px solid  rgba(200,200,200,0.25); 
  box-shadow:0px 2px 2px 2px  rgba(0,0,0,.3) ;
  border-radius: 20px;
  font-size: 14px;
  text-align: center;
  width: 20vw;
  height: 180rpx;
  margin: 0 4.5vw 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  font-weight: 500;
}


.cell-time {
  font-size: 10px;
  color: #808080;
}

image {
  width: 20vw;
  height: 80rpx;
}
